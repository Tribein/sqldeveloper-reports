	<display id="null" type="" style="Chart" enable="true">
		<name><![CDATA[SQL]]></name>
		<description><![CDATA[]]></description>
		<tooltip><![CDATA[]]></tooltip>
		<drillclass><![CDATA[null]]></drillclass>
		<CustomValues>
			<LEGEND>false</LEGEND>
			<STYLE>Default</STYLE>
			<THREED>true</THREED>
			<GRID>true</GRID>
			<GRADIENT>true</GRADIENT>
			<TYPE>BAR_VERT_STACK</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[SELECT TO_CHAR(SAMPLE_TIME,'HH24:MI:SS') "TIME",
  txt,
  COUNT(txt) AS "COUNTER"
FROM gv$active_session_history a
LEFT JOIN
  (SELECT inst_id,
    sql_id,
    plan_hash_value,
    id,
    sql_id
    ||' ('
    ||plan_hash_value
    ||') '
    ||operation
    ||' '
    ||OPTIONS
    ||' '
    ||object_node
    ||' '
    ||' '
    ||object_owner
    ||'.'
    ||object_name
    ||' ('
    ||object_type
    ||' '
    ||object_alias
    ||') '
    ||optimizer txt
  FROM gv$sql_plan
  ) b
ON (a.sql_id             =b.sql_id
AND a.sql_plan_hash_value=b.plan_hash_value
AND a.inst_id            =b.inst_id
AND a.sql_plan_line_id   =b.id)
WHERE SAMPLE_TIME BETWEEN sysdate-1/(24*12) AND sysdate
AND is_sqlid_current          ='Y'
AND NVL(EVENT,'CPU+WAIT')=:EVENT
GROUP BY TO_CHAR(SAMPLE_TIME,'HH24:MI:SS'),
  txt
HAVING COUNT(txt)>0
ORDER BY 1 ASC,
  3 DESC]]></sql>
		</query>
