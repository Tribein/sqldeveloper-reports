	<display id="null" type="" style="Table" enable="true">
		<name><![CDATA[BLOCKING TREE]]></name>
		<description><![CDATA[]]></description>
		<tooltip><![CDATA[]]></tooltip>
		<drillclass><![CDATA[null]]></drillclass>
		<CustomValues>
			<TYPE>horizontal</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[with a as (
select sample_time,session_id,session_SERIAL#,blocking_session,blocking_session_serial#,
username,MODULE,program,MACHINE,session_state,time_waited,event,current_obj#,seq#,p1text,p1,p2text,p2,p3text,p3 from dba_hist_active_sess_history
left join (select user_id,username from dba_users) using (user_id)
where sample_time between trunc(sysdate) and sysdate and to_char(sample_time,'HH24:MI:SS')=:TS
)
select to_char(sample_time,'YYYYMMDD HH24:MI:SS') ts,RPAD('  ',4*level,'  ')||session_id sid,session_SERIAL#,
username,nvl(MODULE,program) progrm,MACHINE,
session_state,round(time_waited/1000) time_waited_milli,nvl(event,'on cpu') evnt,current_obj#,seq#,
p1text,p1,p2text,p2,p3text,p3
from a
start with session_id||','||session_serial#=:BLOCKER
connect by nocycle prior trim(session_id||','||session_serial#)=trim(blocking_session||','||blocking_session_serial#)]]></sql>
