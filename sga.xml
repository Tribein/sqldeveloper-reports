			<GRADIENT>true</GRADIENT>
			<TYPE>BAR_VERT_CLUST</TYPE>
		</CustomValues>
		<query>
			<sql><![CDATA[select 
case 
  when dirty  = 'Y' then 'DIRTY'
  when temp   = 'Y' then 'TEMP'
  when ping   = 'Y' then 'PING'
  when stale  = 'Y' then 'STALE'
  when direct = 'Y' then 'DIRECT'
  else NULL
end "ATTR1",
case 
  when dirty  = 'Y' then 'DIRTY'
  when temp   = 'Y' then 'TEMP'
  when ping   = 'Y' then 'PING'
  when stale  = 'Y' then 'STALE'
  when direct = 'Y' then 'DIRECT'
  else NULL
end "ATTR2",
count(1) "COUNT"
from v$bh where dirty<>'N' or  temp<>'N' or ping<>'N' or stale<>'N' or direct<>'N'
group by 'BUFFER', 
case when dirty='Y' then 'DIRTY' when temp='Y' then 'TEMP' when ping='Y' then 'PING' when stale='Y' then 'STALE' when direct='Y' then 'DIRECT' else NULL end]]></sql>
		</query>
